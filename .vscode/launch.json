{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI (Poetry, sem reload)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.presentation.main:app",
        "--host", "127.0.0.1",
        "--port", "8001"
      ],
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
